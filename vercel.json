{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/home"
        },
        {
            "src": "/home",
            "dest": "/views/home.ejs"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
  }
  